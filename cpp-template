        T = _T = -1;
        while (!cin.eof()) { solve(T - _T + 1); _T--; };
    } else {
        TEST_CASES ? cin >> T : (T = 1, cin);
        _T = T;
        while ((_T--) > 0) solve(T - _T);
    }

    return 0;
}

